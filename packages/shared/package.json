{
  "name": "@daesite/shared",
  "version": "0.1.0",
  "description": "dae shared",
  "license": "AGPL-3.0",
  "author": "Dmitriy Chukhno <me@dmitr1sdae.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "pretty": "prettier --check $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)",
    "pretty:fix": "prettier --write $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)"
  },
  "dependencies": {
    "@daesite/crypto": "workspace:^",
    "@daesite/utils": "workspace:^",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.21",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@daesite/eslint-config-daelint": "workspace:^",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.3",
    "@types/ua-parser-js": "^0.7.39",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "typescript": "5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2"
  }
}
